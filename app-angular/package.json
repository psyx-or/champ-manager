{
  "name": "app-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-admin": "ng serve admin --proxy-config proxy.conf.json",
    "admin-local": "ng build admin --configuration=test --base-href /champ-manager/ --deploy-url /champ-manager/admin/",
    "admin-remote": "ng build admin --prod --base-href /admin/ --deploy-url /admin/",
    "start-user": "ng serve user --proxy-config proxy.conf.json --port 4201",
    "user-local": "ng build user --configuration=test --base-href /champ-manager/ --deploy-url /champ-manager/user/",
    "user-remote": "ng build user --prod --base-href / --deploy-url /user/",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "ionicons": "^4.6.2",
    "moment": "^2.29.0",
    "raphael": "^2.2.8",
    "rxjs": "^6.6.3",
    "treant-js": "^1.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@angular/localize": "^10.2.0",
    "@types/googlemaps": "^3.40.1",
    "@types/node": "^12.19.1",
    "codelyzer": "^5.1.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
