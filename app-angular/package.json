{
  "name": "app-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-admin": "ng serve admin --proxy-config proxy.conf.json",
    "admin-local": "ng build admin --configuration=test --base-href /champ-manager/ --deploy-url /champ-manager/admin/",
    "admin-remote": "ng build admin --prod --base-href /admin/ --deploy-url /admin/",
    "start-user": "ng serve user --proxy-config proxy.conf.json --port 4201",
    "user-local": "ng build user --configuration=test --base-href /champ-manager/ --deploy-url /champ-manager/user/",
    "user-remote": "ng build user --prod --base-href / --deploy-url /user/",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.0",
    "ionicons": "^4.6.2",
    "moment": "^2.24.0",
    "raphael": "^2.2.8",
    "rxjs": "^6.5.3",
    "treant-js": "^1.0.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/googlemaps": "^3.37.7",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "~3.8.3"
  }
}
